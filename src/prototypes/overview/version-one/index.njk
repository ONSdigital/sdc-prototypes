---
group: RAS/RM - ROps
title: Overview
version: 14.2.1
layout: overview/views/_master
---

{% from "components/button/_macro.njk" import onsButton %}
{% from "components/input/_macro.njk" import onsInput %}

{% block main %}

  <div class="grid overview">
    <div class="grid__col col-12@m">
      <div class="panel panel--info panel--simple">
        <div class="panel__body">
          <p>Scheduled downtime 18/12/2019 00:00 to 02:00</p>
        </div>
      </div>
      <h1 name="page-surveys-title" class="u-fs-xxl u-mt-m">MBS <span class="u-fs-r">
          <a href="{{ pageInfo.rootPath }}/version-one/change-survey.html">Change survey</a></span></h1>
    </div>

    <div class="grid__col col-6@m">
      <dl class="metadata metadata__list grid grid--gutterless u-cf u-mb-s" title="Survey information" aria-label="Survey information">
        <dt class="metadata__term grid__col col-4@m">Current period:</dt>
        <dd class="metadata__value grid__col col-8@m">December 2019</dd>
        <dt class="metadata__term grid__col col-4@m">Next key date:</dt>
        <dd class="metadata__value grid__col col-8@m">Second reminder</dd>
        <dd class="metadata__value grid__col col-8@m">Thursday 16 Jan 2020</dd>
      </dl>
    </div>

    <div class="grid__col col-6@m">
      <dl class="metadata metadata__list grid grid--gutterless u-cf u-mb-s" title="Survey information" aria-label="Survey information">
        <dt class="metadata__term grid__col col-4@m">Completed:</dt>
        <dd class="metadata__value grid__col col-8@m">2,114 (24.5%)</dd>
        <dt class="metadata__term grid__col col-4@m">Sample size:</dt>
        <dd class="metadata__value grid__col col-8@m">33,411</dd>
        <dt class="metadata__term grid__col col-4@m">Not started:</dt>
        <dd class="metadata__value grid__col col-8@m">10,867</dd>
        <dt class="metadata__term grid__col col-4@m">In progress:</dt>
        <dd class="metadata__value grid__col col-8@m">375</dd>
      </dl>
      <p class="u-fs-s">Data from the <a href="#">SDC dashboard</a>
      </p>
    </div>

    <div class="grid__col col-5@m">
      <section class="ce-section ce-details">
        <h3 class="table-heading u-fs-r--b u-mb-no u-mt-l">Reference information for this collection exercise</h3>
        <table class="table table--dense" id="tbl-ce-details" summary="Collection exercise details">
          <tbody class="table__body">
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Period </td>
              <td class="table__cell tbl-ce-value" name="period"> 1912 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Shown to respondent as </td>
              <td class="table__cell tbl-ce-value" name="user-description" colspan="2"> December 2019 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Survey ID and Abbreviation </td>
              <td class="table__cell tbl-ce-value" name="survey-info"> 009 MBS </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Survey title </td>
              <td class="table__cell tbl-ce-value" name="survey-title"> Monthly Business Survey </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Reference period start date </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="period-start-date"> Sunday <em>01 Dec 2019</em>
              </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Reference period end date </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="period-end-date"> Tuesday <em>31 Dec 2019</em>
              </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Employment date </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="employment-date"> Saturday <em>14 Dec 2019</em>
              </td>
            </tr>
          </tbody>
        </table>
      </section>
    </div>

    <div class="grid__col col-5@m push-1@m">
      <section class="ce-section ce-events">
        <h3 class="table-heading u-fs-r--b u-mb-no  u-mt-l">Key Dates</h3>
        <table class="table table--dense" summary="Dates for this collection exercise">
          <thead class="table__head u-vh">
            <tr class="table__row">
              <th scope="col" class="table__header tbl-ce-label"> Event </th>
              <th scope="col" class="table__header tbl-ce-value"> Date </th>
            </tr>
          </thead>
          <tbody class="table__body">
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> MPS (Main print selection) </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="mps-date"> Thursday <em>12 Dec 2019</em> 00:00 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Go live </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="go-live-date"> Friday <em>13 Dec 2019</em> 00:00 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Return by </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="return-by-date"> Tuesday <em>07 Jan 2020</em> 00:00 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> First reminder </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="first-reminder-date"> Wednesday <em>08 Jan 2020</em> 00:00 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Second reminder </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="second-reminder-date">Thursday <em>16 Jan 2020</em> 00:00 </td>
            </tr>
            <tr class="table__row">
              <td class="table__cell tbl-ce-label"> Exercise end </td>
              <td class="table__cell tbl-ce-value ce-event-date" name="exercise-end-date"> Monday <em>28 Feb 2022</em> 00:00 </td>
            </tr>
          </tbody>
        </table>
      </section>
    </div>
  </div>

{% endblock %}
