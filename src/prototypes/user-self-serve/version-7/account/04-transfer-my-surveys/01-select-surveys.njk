---
group: RAS/RM - Frontstage
title: User account creation
version: 30.0.0
layout: user-self-serve/views/version-7/_master-survey
---

{% from "components/input/_macro.njk" import onsInput %}
{% from "components/breadcrumb/_macro.njk" import onsBreadcrumb %}
{% from "components/checkboxes/_macro.njk" import onsCheckboxes %}

{% block main %}

    <div class="grid">
        <div id="main" class="grid__col col-12@m u-pt-no u-pb-no">
            {{ 
                onsBreadcrumb({
                    "ariaLabel": 'Breadcrumb',
                    "itemsList": [
                        {
                            "url": '/prototypes/user-self-serve/version-7/00-surveys-list.html',
                            "text": 'Surveys'
                        },
                        {
                            "url": '/prototypes/user-self-serve/version-7/30-my-account.html',
                            "text": 'Account'
                        },
                        {
                            "url": '/prototypes/user-self-serve/version-7/account/04-transfer-my-surveys/index.html',
                            "text": 'Transfer surveys'
                        }
                    ]
                }) 
            }}
        </div>
        <div class="grid__col col-8@m u-pt-no">
            <h1>Select surveys to transfer</h1>

            <p class="field u-mt-l">
                <button type="submit" class="btn btn--secondary btn--small">
                    <span class="btn__inner">Select all surveys</span>
                </button>
            </p>

            {{
                onsCheckboxes({
                    "legend": "ACME Consumer Products",
                    "checkboxesLabel": "Select all that apply",
                    "name": "ACME Consumer Products",
                    "checkboxes": [
                        {
                            "id": "Quarterly Business Survey",
                            "label": {
                                "text": "Quarterly Business Survey"
                            },
                            "value": "Quarterly Business Survey"
                        },
                        {
                            "id": "Monthly Business Survey",
                            "label": {
                                "text": "Monthly Business Survey"
                            },
                            "value": "Monthly Business Survey"
                        }
                    ]
                })
            }}

            {{
                onsCheckboxes({
                    "legend": "ACME Group Ltd",
                    "checkboxesLabel": "Select all that apply",
                    "name": "ACME Group Ltd",
                    "checkboxes": [
                        {
                            "id": "BRSS",
                            "label": {
                                "text": "Business Register and Employment Survey"
                            },
                            "value": "BRSS"
                        },
                        {
                            "id": "MBS",
                            "label": {
                                "text": "Monthly Business Survey"
                            },
                            "value": "MBS"
                        },
                        {
                            "id": "QBS",
                            "label": {
                                "text": "Quarterly Business Survey"
                            },
                            "value": "QBS"
                        },
                        {
                            "id": "MWSS",
                            "label": {
                                "text": "Monthly Wages and Salary Survey"
                            },
                            "value": "MWSS"
                        },
                        {
                            "id": "QADCA",
                            "label": {
                                "text": "Quarterly Acquisitions and Disposals of Capital Assets"
                            },
                            "value": "QADCA"
                        }
                    ]
                })
            }}
            <p class="field u-mt-l">
                <button type="submit" class="btn" onclick="location.href='{{ pageInfo.rootPath }}/version-7/account/04-transfer-my-surveys/02-email-address.html';">
                    <span class="btn__inner">Continue</span>
                </button>
                <button type="submit" class="btn btn--secondary" onclick="location.href='{{ pageInfo.rootPath }}/version-7/account/04-transfer-my-surveys/index.html';">
                    <span class="btn__inner">Cancel</span>
                </button>
            </p>

        </div>
    </div>

{% endblock %}
