{% from "components/button/_macro.njk" import onsButton %}
{% from "components/textarea/_macro.njk" import onsTextarea %}
{% from "components/input/_macro.njk" import onsInput %}

{% block main %}

    <h1 name="page-surveys-title">Load collection instruments for 001 NEW</h1>
    <div class="grid overview">
        <div class="grid__col col-6@m">
            <p class="field">
                <label class="label u-fs-m" for="file">Upload SEFT files
                        <br>
                    <span class="label__description">Only .xlsx file type is accepted</span>
                </label>
                <input type="file" id="file" class="input input--text input-type__input  input--upload input--w-20" accept=".xls,.xlsx"/>
            </p>
            <p class="field">
                <form action="" method="get">
                    {{ 
                    onsButton({
                        "text": 'Upload',
                        "url": 'load-collection-instruments-checking.html',
                        "classes": 'btn--secondary btn--small'
                    }) 
                }}
                </form>
            </p>
            <h2 class="u-fs-m u-mt-l u-mb-no">3 collection instruments</h3>
            <table class="table  table--sortable" data-aria-sort="Sort by" data-aria-asc="ascending" data-aria-desc="descending">
                <thead class="table__head">
                    <tr class="table__row">
                        <th scope="col" class="table__header">File name</th>
                        <th scope="col" class="table__header"></th>
                    </tr>
                </thead>
                <tbody class="table__body">
                    <tr class="table__row">
                        <td class="table__cell " name="tbl-ci-id" data-th="ID">001_0255.xlsx</td>
                        <td class="table__cell u-ta-right" name="tbl-ci-id" data-th="ID">
                            <button type="submit" class="btn btn--secondary btn--small" id="remove">
                                <span class="btn__inner">Remove</span>
                            </button>
                        </td>
                    </tr>
                    <tr class="table__row">
                        <td class="table__cell " name="tbl-ci-id" data-th="ID">001_0873.xlsx</td>
                        <td class="table__cell u-ta-right" name="tbl-ci-id" data-th="ID">
                            <button type="submit" class="btn btn--secondary btn--small" id="remove">
                                <span class="btn__inner">Remove</span>
                            </button>
                        </td>
                    </tr>
                    <tr class="table__row">
                        <td class="table__cell " name="tbl-ci-id" data-th="ID">001_0867.xlsx</td>
                        <td class="table__cell u-ta-right" name="tbl-ci-id" data-th="ID">
                            <button type="submit" class="btn btn--secondary btn--small" id="remove">
                                <span class="btn__inner">Remove</span>
                            </button>
                        </td>
                    </tr>
                </tbody>
            </table>
