{% extends "page-templates/_template.njk" %}

{% set pageConfig = {
    "cdn": {
      "version": '44.0.0'
    },
    "header": {
        "logoHref": '/',
        "titleLogoHref": pageInfo.rootPath + '#0'
    },
    "title": "Prototype",
    "footer": {
        "OGLLink": {
            "pre": 'All content is available under the',
            "link": 'Open Government Licence v3.0',
            "url": 'https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/',
            "post": ', except where otherwise stated'
        }
    }
} %}

{% block head %}
    <link rel="stylesheet" href="{{ pageInfo.rootPath }}/styles.css" />
{% endblock %}

{% block preMain %}
  <nav class="ons-breadcrumb" aria-label="Back">
    <ol class="ons-breadcrumb__items ons-u-fs-s">
      <li class="ons-breadcrumb__item">
        <a class="ons-breadcrumb__link js-previous" href="{{ pageInfo.rootPath }}/{{ prevPage }}.html">{{ backLink | default ('Previous') }}</a>
        <svg class="ons-svg-icon " viewBox="0 0 8 13" xmlns="http://www.w3.org/2000/svg" focusable="false" fill="currentColor">
          <path d="M5.74,14.28l-.57-.56a.5.5,0,0,1,0-.71h0l5-5-5-5a.5.5,0,0,1,0-.71h0l.57-.56a.5.5,0,0,1,.71,0h0l5.93,5.93a.5.5,0,0,1,0,.7L6.45,14.28a.5.5,0,0,1-.71,0Z" transform="translate(-5.02 -1.59)" />
        </svg>
      </li>
    </ol>
  </nav>
{% endblock %}

{% block main %}
{% endblock %}

{% block bodyEnd %}
    <script src="{{ pageInfo.rootPath }}/scripts/index.js"></script>
{% endblock %}
