---
title: Thank you for contacting us
group: Census
version: 21.0.0
layout: census-website-engage/views/_rh-master
---

{% block main %}

    <div class="page__container container">

        {{ 
            onsBreadcrumb({
                "ariaLabel": 'Breadcrumb',
                "itemsList": [
                    {
                        "url": pageInfo.rootPath + '/',
                        "text": 'Home'
                    },
                    {
                        "url": pageInfo.rootPath + '/web-form',
                        "text": 'Web form'
                    }
                ]
            }) 
        }}

        <div class="grid">

            <div class="grid__col col-12@m">

                <main class="page__main" id="main-content" role="main">

                    <div class="col-8@m u-mb-l">

                        {% from "components/panel/_macro.njk" import onsPanel %}
                            
                            {% call onsPanel({
                                    type: 'success',
                                    classes: 'u-mb-m'
                               })
                            %}

                            <h1 class="u-fs-xxl">Thank you for contacting us</h1>

                            <p>You message has been sent.</p>

                        {% endcall %}

                        <p>We will respond to your message within 48 working hours.</p>

                    </div>

                </main>

            </div>

        </div>

    </div>

{% endblock %}
