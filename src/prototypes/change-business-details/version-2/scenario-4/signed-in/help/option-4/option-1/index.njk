{% from "components/button/_macro.njk" import onsButton %}
{% from "components/radios/_macro.njk" import onsRadios %}
{% from "components/textarea/_macro.njk" import onsTextarea %}
{% from "components/metadata/_macro.njk" import onsMetadata %}
{% from "components/breadcrumb/_macro.njk" import onsBreadcrumb %}

{% block main %}

    <div class="grid">
        <div id="main" class="grid__col col-12@m u-pt-no u-pb-no">
            {{ 
                onsBreadcrumb({
                    "ariaLabel": 'Breadcrumb',
                    "itemsList": [
                        {
                            "url": '/prototypes/change-business-details/version-2/scenario-4/signed-in/00-surveys-list.html',
                            "text": 'Surveys'
                        },
                        {
                            "url": '/prototypes/change-business-details/version-2/scenario-4/signed-in/help/index.html',
                            "text": 'Help'
                        },
                        {
                            "url": '/prototypes/change-business-details/version-2/scenario-4/signed-in/help/option-4/option-list.html',
                            "text": 'Changes to your account'
                        }
                    ]
                }) 
            }}
        </div>
        <div class="grid__col col-8@m u-pt-no">
            <h1>I have two accounts in error</h1>
            <p>This may have occurred if you have received a new survey enrolment and set up a new account to complete it.</p>
 
            <p>If this is the case, you should <a href="/prototypes/change-business-details/version-2/scenario-4/signed-in/account/04-transfer-my-surveys/index.html">transfer the enrolment your correct account</a>.</h2>
            <p class="field">
                <div class="btn-group">
                    {{ 
                        onsButton({
                            "type": 'button',
                            "text": 'Yes',
                            "classes": 'btn-group__btn',
                            "url": "/prototypes/change-business-details/version-2/scenario-4/signed-in/00-surveys-list.html"
                        }) 
                    }}
                    {{ 
                        onsButton({
                            "type": 'button',
                            "text": 'No',
                            "classes": 'btn--secondary btn-group__btn',
                            "url": "send-message.html"
                        }) 
                    }}
                </div>
            </p>
        </div>
    </div>

{% endblock %}
